<scenario name="pfcp-setup" description="PFCP client setup">
    <group name="Pfcp-Setup">
        <step name="Pfcp-Client-Start"
              exec="pfcp-client-cmd start"/>
        <step name="Check-Pfcp-Client-Survived-Start" requires="^"
              exec="pfcp-client-cmd check"/>
        <step name="Pfcp-Client-Associate" requires="^"
              exec="pfcp-client-cmd associate"/>
        <step name="Check-Pfcp-Agent-Survived-Start" requires="^" delay="1"
              exec="pfcp-agent-check-alive"/>
    </group>
</scenario>
